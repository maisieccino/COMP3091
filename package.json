    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.4",
    "prettier": "^1.12.0",
    "supertest": "^3.0.0"
  },
  "scripts": {
    "lint": "eslint . --ext .js --ext .jsx --cache --ignore-path .eslintignore",
